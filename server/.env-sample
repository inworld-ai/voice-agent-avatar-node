# DO NOT EDIT THIS FILE DIRECTLY
# Copy this file to .env and enter your own values
#
# INWORLD_API_KEY is required
INWORLD_API_KEY=
# VAD_MODEL_PATH is optional, defaults to packaged https://github.com/snakers4/silero-vad
VAD_MODEL_PATH=
# LLM_MODEL_NAME is optional, defaults to `gpt-4o-mini`
LLM_MODEL_NAME=
# LLM_PROVIDER is optional, defaults to `openai`
LLM_PROVIDER=

# Default Voice and Avatar (fallback for unmatched agents)
DEFAULT_VOICE_ID=Dennis
DEFAULT_HEYGEN_AVATAR_ID=e9844e6d-847e-4964-a92b-7ecd066f69df

# Female Voice and Avatar (used when female gender is detected in custom prompts)
DEFAULT_FEMALE_VOICE_ID=Ashley
DEFAULT_FEMALE_HEYGEN_AVATAR_ID=c72a9099-84b9-4d5d-98f4-a19ba131e654

# Santa Claus (Fantasy Character template)
VOICE_ID_SANTA_CLAUS=Craig
HEYGEN_AVATAR_ID_SANTA_CLAUS=1c690fe7-23e0-49f9-bfba-14344450285b

# Coach Dennis (Fitness Coach template)
VOICE_ID_COACH_DENNIS=Carter
HEYGEN_AVATAR_ID_COACH_DENNIS=55eec60c-d665-4972-a529-bbdcaf665ab8

# Riley (AI Companion template)
VOICE_ID_RILEY=Hana
HEYGEN_AVATAR_ID_RILEY=65f9e3c9-d48b-4118-b73a-4ae2e3cbb8f0

# TTS_MODEL_ID is optional, defaults to `inworld-tts-1`
TTS_MODEL_ID=
# If enabled, it will be saved in system tmp folder.
# Path will be printed in CLI on application start.
# Default value is `false`, set `true` to enable this feature
GRAPH_VISUALIZATION_ENABLED=
# STT (Speech-to-Text) Configuration
# Assembly.AI is the required STT service for this application
# ASSEMBLY_AI_API_KEY is required
ASSEMBLY_AI_API_KEY=
# Heygen API Key is required
# Used for HeyGen Live Avatar video display with lip-sync
HEYGEN_API_KEY=